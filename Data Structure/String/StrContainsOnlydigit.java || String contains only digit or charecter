import java.util.Scanner;

public class StrContainsOnlydigit {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter string : ");
        String str = sc.nextLine();

        if (strMethod(str))
            System.out.println("String contains only digit");
        else
            System.out.println("String contains non-digit characters");

    }

    public static boolean strMethod(String str) {
        for (int i = 0; i < str.length(); i++) {
            if(str.charAt(i) > '0' && str.charAt(i) < '9')
            {
                continue;
            }
            else{
                return false;
            }
        }
        return true;
    }
}


/*

### ðŸ§© **Problem Statement: Check if a String Contains Only Digits**

